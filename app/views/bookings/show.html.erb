<div class="page-header">
  <%= link_to bookings_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_booking_path(@booking), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show booking</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @booking.name %></dd>

  <dt>Email:</dt>
  <dd><%= @booking.email %></dd>

  <dt>Cc number:</dt>
  <dd><%= @booking.cc_number %></dd>

  <dt>Cc cvc:</dt>
  <dd><%= @booking.cc_cvc %></dd>

  <dt>Cc exp mon:</dt>
  <dd><%= @booking.cc_exp_mon %></dd>

  <dt>Cc exp yr:</dt>
  <dd><%= @booking.cc_exp_yr %></dd>

  <dt>Buyer age confirmed:</dt>
  <dd><%= @booking.buyer_age_confirmed %></dd>

  <dt>Total cost:</dt>
  <dd><%= @booking.total_cost %></dd>

</dl>
