<div class="row">
  <div class="col-xs-12 col-md-10 col-md-offset-1">
    <div class="movie-app-page-header">
      <h1>Checkout</h1>
    </div>
  </div>
</div>
<br>
<div class="row">
  <div class="col-xs-12 col-md-10 col-md-offset-1">
    <div class="row">
      <div class="col-md-2">
        <%= render 'movie_showing_information' %>
      </div>
      <%= form_tag(new_booking_url, method: :get) do %>
        <%= hidden_field_tag :showing_id, @showing.id %>
        <div class="col-md-6">
          <%= render 'ticket_selection' %>
        </div>
        <div class="col-md-4">
          <%= render 'checkout_box' %>
        </div>
      <% end %>
    </div>
  </div>
</div>

