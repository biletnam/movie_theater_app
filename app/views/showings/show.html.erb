<div class="page-header">
  <%= link_to showings_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_showing_path(@showing), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show showing</h1>
</div>

<dl class="dl-horizontal">
  <dt>Time slot:</dt>
  <dd><%= @showing.time_slot %></dd>

  <dt>Seats available:</dt>
  <dd><%= @showing.seats_available %></dd>

  <dt>Screening room:</dt>
  <dd><%= @showing.screening_room_id %></dd>

  <dt>Movie:</dt>
  <dd><%= @showing.movie_id %></dd>

</dl>
