<div class="page-header">
  <%= link_to ticket_offerings_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_ticket_offering_path(@ticket_offering), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show ticket_offering</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @ticket_offering.name %></dd>

  <dt>Price:</dt>
  <dd><%= @ticket_offering.price %></dd>

  <dt>Movie theater:</dt>
  <dd><%= @ticket_offering.movie_theater_id %></dd>

</dl>
