<div class="page-header">
  <%= link_to screening_rooms_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_screening_room_path(@screening_room), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show screening_room</h1>
</div>

<dl class="dl-horizontal">
  <dt>Theater number:</dt>
  <dd><%= @screening_room.theater_number %></dd>

  <dt>Seat quantity:</dt>
  <dd><%= @screening_room.seat_quantity %></dd>

  <dt>Movie theater:</dt>
  <dd><%= @screening_room.movie_theater_id %></dd>

</dl>
