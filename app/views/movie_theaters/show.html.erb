<div class="page-header">
  <%= link_to movie_theaters_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_movie_theater_path(@movie_theater), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show movie_theater</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @movie_theater.name %></dd>

  <dt>Address 1:</dt>
  <dd><%= @movie_theater.address_1 %></dd>

  <dt>Address 2:</dt>
  <dd><%= @movie_theater.address_2 %></dd>

  <dt>City:</dt>
  <dd><%= @movie_theater.city %></dd>

  <dt>State:</dt>
  <dd><%= @movie_theater.state %></dd>

  <dt>Zip:</dt>
  <dd><%= @movie_theater.zip %></dd>

  <dt>Phone:</dt>
  <dd><%= @movie_theater.phone %></dd>

</dl>
